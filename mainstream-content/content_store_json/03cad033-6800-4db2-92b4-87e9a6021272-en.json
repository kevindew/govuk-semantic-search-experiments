      {
        "api_path": "/api/content/get-free-childcare-if-youre-working",
        "api_url": "https://www.gov.uk/api/content/get-free-childcare-if-youre-working",
        "base_path": "/get-free-childcare-if-youre-working",
        "content_id": "f517cd57-3c18-4bb9-aa8b-1b907e279bf9",
        "details": {
          "step_by_step_nav": {
            "steps": [
              {
                "contents": [
                  {
                    "contents": [
                      {
                        "href": "/check-eligible-free-childcare-if-youre-working",
                        "text": "Check if you're eligible for free childcare if you're working"
                      },
                      {
                        "href": "/help-with-childcare-costs",
                        "text": "Find out what types of childcare you can use"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Check you're eligible"
              },
              {
                "contents": [
                  {
                    "text": "Getting free childcare might affect how much you get in tax credits.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/childcare-calculator",
                        "text": "Check if you’ll be better off"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Check if you'll be better off"
              },
              {
                "contents": [
                  {
                    "text": "There's one application for free childcare if you're working and Tax-Free Childcare. As part of your application, you will find out if you can get both.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/apply-free-childcare-if-youre-working",
                        "text": "Apply online"
                      }
                    ],
                    "type": "list"
                  },
                  {
                    "text": "If you're eligible you'll get a childcare account and a code for free childcare.",
                    "type": "paragraph"
                  }
                ],
                "title": "Apply for free childcare if you're working"
              },
              {
                "contents": [
                  {
                    "text": "Give the code in your childcare account to your provider along with:",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "text": "your National Insurance number"
                      },
                      {
                        "text": "your child's date of birth"
                      }
                    ],
                    "style": "choice",
                    "type": "list"
                  }
                ],
                "title": "Give your code to your childcare provider"
              },
              {
                "contents": [
                  {
                    "text": "You must confirm your details are up to date every 3 months or your code will expire.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/sign-in-childcare-account",
                        "text": "Sign in to your childcare account to confirm your details"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Sign in to your account to reconfirm your details"
              }
            ],
            "title": "Get free childcare if you're working: step by step"
          }
        },
