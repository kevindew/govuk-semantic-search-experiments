        "details": {
          "step_by_step_nav": {
            "steps": [
              {
                "contents": [
                  {
                    "text": "You must update your provisional or full driving licence when you move.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/change-address-driving-licence",
                        "text": "Change the address on your driving licence"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Update your driving licence"
              },
              {
                "contents": [
                  {
                    "text": "You must update a vehicle's log book if you're the registered keeper.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/change-address-v5c",
                        "text": "Change the address on your vehicle's log book"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Update your vehicle's log book (V5C)"
              },
              {
                "contents": [
                  {
                    "text": "If you pay for your vehicle tax by Direct Debit, you need to tell DVLA when you change address.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/contact-the-dvla/y/vehicle-tax-and-sorn",
                        "text": "Phone DVLA to tell them you've moved"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Update your Direct Debit"
              },
              {
                "contents": [
                  {
                    "text": "If you have a private registration number and it's not on a vehicle, you must update your V750 or V778 documents.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/personalised-vehicle-registration-numbers/change-your-name-and-address",
                        "text": "Update your personalised registration documents"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Update your personalised number plate documents"
              },
              {
                "contents": [
                  {
                    "text": "If you have a trailer and want to continue taking it abroad, you must register it to your new address.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/register-trailer-to-take-abroad",
                        "text": "Change the address on your trailer registration certificate"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Update your trailer registration certificate"
              }
            ],
            "title": "Tell DVLA you've changed address: step by step"
          }
