    "external_related_links": [

    ],
    "introductory_paragraph": "<p>Find registered local bus services by operator name, licence number, or start and finish point.</p>\n",
    "more_information": "<p>A <a href=\"/run-local-bus-service\">local bus service</a> carries passengers who pay separate fares over short distances.</p>\n\n<div role=\"note\" aria-label=\"Information\" class=\"application-notice info-notice\">\n<p>There are different services to <a href=\"/find-vehicle-operators\">find bus operators</a> and <a href=\"/check-vehicle-operator-licence-applications\">check bus operator applications</a>.</p>\n</div>\n",
    "start_button_text": "Start now",
    "transaction_start_link": "https://www.vehicle-operator-licensing.service.gov.uk/search/find-registered-local-bus-services",
    "will_continue_on": ""
  },
