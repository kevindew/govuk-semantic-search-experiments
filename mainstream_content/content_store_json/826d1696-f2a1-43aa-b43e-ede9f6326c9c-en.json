      {
        "api_path": "/api/content/buy-a-vehicle",
        "api_url": "https://www.gov.uk/api/content/buy-a-vehicle",
        "base_path": "/buy-a-vehicle",
        "content_id": "58fad183-27f5-4dd9-b51e-696c992373d7",
        "details": {
          "step_by_step_nav": {
            "steps": [
              {
                "contents": [
                  {
                    "text": "You'll need the vehicle's registration number, make, model and MOT test number. You also need to see the V5C vehicle registration certificate (log book). ",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/get-vehicle-information-from-dvla",
                        "text": "Check the details you've been given match DVLA's information"
                      },
                      {
                        "href": "/check-mot-history",
                        "text": "Check the vehicle's MOT history"
                      },
                      {
                        "href": "/check-vehicle-recall",
                        "text": "Check if the vehicle has been recalled because of a safety issue"
                      },
                      {
                        "href": "/checks-when-buying-a-used-car",
                        "text": "Check the log book isn't for a stolen vehicle"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "optional": false,
                "title": "Check the vehicle before you buy it"
              },
              {
                "contents": [
                  {
                    "text": "Once you've bought the vehicle, you have to register it. How you register it depends on whether it has a V5C registration certificate (log book).",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/vehicle-registration/new-and-used-vehicles",
                        "text": "Find out how to register your vehicle"
                      },
                      {
                        "context": "£25",
                        "href": "/vehicle-log-book",
                        "text": "Get a vehicle log book if you don't have it"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "optional": false,
                "title": "Buy and register the vehicle"
              },
              {
                "contents": [
                  {
                    "text": "You must get an MOT for your vehicle every year once it's 3 years old. ",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/mot-reminder",
                        "text": "Get a reminder when your MOT is due"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "logic": "and",
                "optional": true,
                "title": "Register for MOT reminders"
              },
              {
                "contents": [
                  {
                    "text": "You must have insurance before you can use your vehicle on the road.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "https://ownvehicle.askmid.com/",
                        "text": "Check if your vehicle is insured on the Motor Insurance Database"
                      },
                      {
                        "href": "/vehicle-insurance/uninsured-vehicles",
                        "text": "Read about vehicle insurance"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "optional": false,
                "title": "Insure the vehicle"
              },
              {
                "contents": [
                  {
                    "text": "You need to tax the vehicle before you can use it on the road.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/vehicle-tax-rate-tables",
                        "text": "Check how much you’ll have to pay to tax your vehicle"
                      },
                      {
                        "href": "/vehicle-exempt-from-vehicle-tax",
                        "text": "Check when you don't have to pay vehicle tax"
                      },
                      {
                        "href": "/vehicle-tax",
                        "text": "Tax your vehicle"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "optional": false,
                "title": "Tax the vehicle"
              }
            ],
            "title": "Buy a vehicle: step by step"
          }
        },
