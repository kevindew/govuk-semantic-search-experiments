        "content_id": "01ff8dbd-886a-4dbb-872c-d2092b31b2cf",
        "details": {
          "step_by_step_nav": {
            "steps": [
              {
                "contents": [
                  {
                    "contents": [
                      {
                        "href": "/working-for-yourself",
                        "text": "Check what being self-employed means"
                      }
                    ],
                    "type": "list"
                  },
                  {
                    "text": "There are other ways to work for yourself. Check if you should set up as one of the following instead:",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/limited-company-formation",
                        "text": "a limited company"
                      },
                      {
                        "href": "/set-up-business-partnership",
                        "text": "a partner in a business partnership"
                      }
                    ],
                    "style": "choice",
                    "type": "list"
                  },
                  {
                    "contents": [
                      {
                        "href": "/business-support-helpline",
                        "text": "Get help deciding how to set up your business"
                      },
                      {
                        "href": "/moving-from-benefits-to-work/starting-your-own-business",
                        "text": "Get help starting your own business if you're on benefits"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Check if being self-employed is right for you"
              },
              {
                "contents": [
                  {
                    "contents": [
                      {
                        "href": "/set-up-sole-trader",
                        "text": "Check the rules for sole trader business names"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Choose the name you want to trade under"
              },
              {
                "contents": [
                  {
                    "contents": [
                      {
                        "href": "/self-employed-records",
                        "text": "Find out what business records you need to keep"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Check what records you'll need to keep"
              },
              {
                "contents": [
                  {
                    "text": "To pay tax, you'll need to register for Self Assessment.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/apply-national-insurance-number",
                        "text": "Apply for a National Insurance number if you do not have one"
                      },
                      {
                        "href": "/register-for-self-assessment",
                        "text": "Register for Self Assessment"
                      }
                    ],
                    "type": "list"
                  },
                  {
                    "text": "You might need to register in a different way if:",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/what-is-the-construction-industry-scheme",
                        "text": "you work in construction"
                      },
                      {
                        "href": "/guidance/share-fisherman-income-tax-and-national-insurance-contributions",
                        "text": "you're a self-employed fisherman"
                      }
                    ],
                    "style": "choice",
                    "type": "list"
                  },
                  {
                    "text": "You may also need to register for VAT.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/vat-registration",
                        "text": "Check if you need to register for VAT"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Register for tax"
              }
            ],
            "title": "Set up as self-employed (a 'sole trader'): step by step"
          }
