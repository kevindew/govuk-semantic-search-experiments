    "related_to_step_navs": [
      {
        "api_path": "/api/content/get-childcare",
        "api_url": "https://www.gov.uk/api/content/get-childcare",
        "base_path": "/get-childcare",
        "content_id": "f237ec8e-e82c-4ffa-8fba-2a88a739783b",
        "details": {
          "step_by_step_nav": {
            "steps": [
              {
                "contents": [
                  {
                    "contents": [
                      {
                        "href": "/childcare-calculator",
                        "text": "Check what help you could get with childcare costs"
                      }
                    ],
                    "type": "list"
                  },
                  {
                    "text": "You can only get help with 'approved childcare'. ",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/help-with-childcare-costs",
                        "text": "Find out what 'approved childcare' is"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Find out if you can get help with childcare costs"
              },
              {
                "contents": [
                  {
                    "contents": [
                      {
                        "href": "/find-nursery-school-place",
                        "text": "Find a nursery school place"
                      },
                      {
                        "href": "/find-registered-childminder",
                        "text": "Find a registered childminder"
                      },
                      {
                        "href": "/find-free-early-education",
                        "text": "Search for free early education and childcare"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "logic": "and",
                "title": "Find where you can get childcare"
              },
              {
                "contents": [
                  {
                    "text": "You can get help with childcare costs for children under 18. You might be able to apply for:",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/get-tax-free-childcare",
                        "text": "Tax-Free Childcare"
                      },
                      {
                        "href": "/help-with-childcare-costs/tax-credits",
                        "text": "tax credits"
                      },
                      {
                        "href": "/help-with-childcare-costs/universal-credit",
                        "text": "Universal Credit"
                      },
                      {
                        "href": "/help-with-childcare-costs/support-while-you-study",
                        "text": "help while you study"
                      }
                    ],
                    "style": "choice",
                    "type": "list"
                  },
                  {
                    "text": "You could also get free hours childcare when your child is aged 2 to 4. You might be able to apply for:",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/help-with-childcare-costs/free-childcare-2-year-olds",
                        "text": "free education and childcare for 2 year olds"
                      },
                      {
                        "href": "/help-with-childcare-costs/free-childcare-and-education-for-2-to-4-year-olds",
                        "text": "15 hours free childcare for 3 to 4 year olds"
                      },
                      {
                        "href": "/get-free-childcare-if-youre-working",
                        "text": "free childcare if you're working"
                      }
                    ],
                    "style": "choice",
                    "type": "list"
                  },
                  {
                    "text": "You can only get childcare vouchers if you have already joined a scheme.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/help-with-childcare-costs/childcare-vouchers",
                        "text": "Check the rules for childcare vouchers"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Get help paying for childcare"
              },
              {
                "contents": [
                  {
                    "text": "Tell government straight away if your circumstances change and you're getting:",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/changes-affect-tax-credits",
                        "text": "tax credits"
                      },
                      {
                        "href": "/universal-credit/changes-of-circumstances",
                        "text": "Universal Credit"
                      }
                    ],
                    "style": "choice",
                    "type": "list"
                  },
                  {
                    "text": "If you're getting free childcare if you're working or Tax-Free Childcare you have to confirm if your details are up to date every 3 months.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/sign-in-childcare-account",
                        "text": "Sign into your childcare account to find out when you next have to check your details"
                      }
                    ],
                    "type": "list"
                  },
                  {
                    "text": "Your eligibility for support with childcare costs might also change. ",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/childcare-calculator",
                        "text": "Check if your eligibility for help with childcare costs has changed"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Check what to do if your circumstances change"
              },
              {
                "contents": [
                  {
                    "contents": [
                      {
                        "href": "/schools-admissions/applying",
                        "text": "Apply for a primary school place"
                      },
                      {
                        "href": "/after-school-holiday-club",
                        "text": "Find before and after school and holiday clubs"
                      }
                    ],
                    "type": "list"
                  },
                  {
                    "text": "You might be able to use help like tax credits or Tax-Free Childcare to pay for childcare like wraparound care, after school clubs or other activities.",
                    "type": "paragraph"
                  }
                ],
                "title": "Check what to do when your child reaches school age"
              }
            ],
            "title": "Get childcare: step by step"
          }
        },
