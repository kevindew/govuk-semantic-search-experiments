    "external_related_links": [

    ],
    "introductory_paragraph": "<p>Report an excessively smoky lorry or bus exhaust to the Driver and Vehicle Standards Agency (<abbr title=\"Driver and Vehicle Standards Agency\">DVSA</abbr>).</p>\n\n<div role=\"note\" aria-label=\"Information\" class=\"application-notice info-notice\">\n<p><a rel=\"external\" href=\"https://www.police.uk/pu/contact-us\">Contact your local police</a> to report other types of smoky vehicle.</p>\n</div>\n\n",
    "start_button_text": "Start now",
    "transaction_start_link": "https://contact-dvsa.service.gov.uk/report-smoky-vehicle",
    "variants": [

    ],
    "will_continue_on": "the report a smoky vehicle service"
