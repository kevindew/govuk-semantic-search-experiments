    "related_to_step_navs": [
      {
        "api_path": "/api/content/challenge-appeal-benefit-decision",
        "api_url": "https://www.gov.uk/api/content/challenge-appeal-benefit-decision",
        "base_path": "/challenge-appeal-benefit-decision",
        "content_id": "6a0f22f3-3292-4e84-a408-f5e5c1400ac9",
        "details": {
          "step_by_step_nav": {
            "steps": [
              {
                "contents": [
                  {
                    "text": "You can get free support and advice from:",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "https://www.citizensadvice.org.uk/about-us/contact-us/contact-us/contact-us/",
                        "text": "Citizens Advice"
                      },
                      {
                        "href": "https://www.advicenow.org.uk/help-deal-benefit-problems",
                        "text": "Advicenow"
                      },
                      {
                        "href": "https://advicelocal.uk/find-an-adviser",
                        "text": "an adviser near you on Advicelocal"
                      }
                    ],
                    "style": "choice",
                    "type": "list"
                  },
                  {
                    "contents": [
                      {
                        "href": "/find-legal-advice/find-legal-adviser",
                        "text": "You can also seek advice from a legal adviser or solicitor"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Find support and advice"
              },
              {
                "contents": [
                  {
                    "contents": [
                      {
                        "href": "/mandatory-reconsideration",
                        "text": "Ask for the benefits decision to be looked at again (mandatory reconsideration)"
                      }
                    ],
                    "type": "list"
                  },
                  {
                    "text": "You normally need to do this within one month of the date on your decision letter.",
                    "type": "paragraph"
                  }
                ],
                "title": "Ask for mandatory reconsideration"
              },
              {
                "contents": [
                  {
                    "text": "If you’re unhappy with the decision after mandatory reconsideration, you can appeal it to a tribunal.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/appeal-benefit-decision",
                        "text": "Appeal to the Social Security and Child Support Tribunal"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "title": "Appeal the decision to a tribunal"
              },
              {
                "contents": [
                  {
                    "text": "If you disagree with the decision, you can ask for it to be set aside (cancelled). The decision letter from the tribunal will tell you how to do this.",
                    "type": "paragraph"
                  }
                ],
                "title": "Ask for the tribunal's decision to be 'set aside' (cancelled)"
              },
              {
                "contents": [
                  {
                    "text": "If you think the decision was wrong for a legal reason, you can appeal to the Upper Tribunal.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/administrative-appeals-tribunal",
                        "text": "Appeal to the Upper Tribunal (Administrative Appeals Chamber)"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "logic": "or",
                "title": "Appeal the tribunal's decision"
              },
              {
                "contents": [
                  {
                    "text": "You may be eligible for legal aid to help you appeal to the Upper Tribunal.",
                    "type": "paragraph"
                  },
                  {
                    "contents": [
                      {
                        "href": "/legal-aid",
                        "text": "Check if you can get legal aid"
                      }
                    ],
                    "type": "list"
                  }
                ],
                "logic": "and",
                "title": "Check if you can get legal aid"
              }
            ],
            "title": "Challenge and appeal a benefit decision: step by step"
          }
        },
